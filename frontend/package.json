{
  "name": "blue-pair",
  "version": "0.0.1",
  "description": "Pair recording application",
  "license": "BSD-3-Clause",
  "author": "NSE(Neuroscientific Software Engineering) <bbp-ou-nse@groupes.epfl.ch> (http://bluebrain.epfl.ch)",
  "contributors": [
    "pavlo.getta@epfl.ch"
  ],
  "homepage": "https://bluebrain.epfl.ch",
  "repository": {
    "type": "git",
    "url": "https://bbpcode.epfl.ch/browse/code/nse/blue-pair/",
    "issuesurl": "NA"
  },
  "scripts": {
    "serve": "vue-cli-service serve --open",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "chroma-js": "^2.0.3",
    "distinct-colors": "^1.0.4",
    "dygraphs": "^2.1.0",
    "file-saver": "^2.0.2",
    "gsap": "^2.1.2",
    "iview": "^3.3.3",
    "localforage": "^1.7.3",
    "lodash": "^4.17.11",
    "orbit-controls-es6": "^1.0.12",
    "qs": "^6.7.0",
    "three": "^0.106.2",
    "vue": "^2.6.10",
    "vue-slider-component": "^2.8.14"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.5.1",
    "@vue/cli-plugin-eslint": "^3.5.1",
    "@vue/cli-service": "^3.5.2",
    "@vue/eslint-config-airbnb": "^4.0.1",
    "babel-eslint": "^10.0.1",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-vue": "^5.2.2",
    "node-sass": "^4.12.0",
    "sass-loader": "^7.1.0",
    "vue-template-compiler": "^2.6.10"
  },
  "babel": {
    "presets": [
      "@vue/app"
    ]
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "plugin:vue/essential",
      "@vue/airbnb"
    ],
    "rules": {
      "indent": "off",
      "vue/script-indent": [
        "error",
        2,
        {
          "baseIndent": 1
        }
      ]
    },
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "settings": {
      "import/resolver": {
        "alias": [
          [
            "@",
            "./frontend/src"
          ]
        ]
      }
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
